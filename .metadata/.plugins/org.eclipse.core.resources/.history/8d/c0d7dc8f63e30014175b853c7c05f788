
public class Inhi 
{

	public static void main(String[] args) 
	{
		Base obj = new Derived() ;
		obj.print();
		
	}

	
	static 	class Base 
	{
		public void print()
		{
			System.out.println("BaseClass");
		}
	}
	
	static class Derived extends Base
	{
		public void print()
		{
			System.out.println("Derived");
		}
	}
}
