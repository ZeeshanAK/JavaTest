
public class Inhi 
{

	public static void main(String[] args) 
	{
		Inhi h = new Inhi();
		
		Base obj = h.new Derived() ;
		obj.print();
		
	}

	
	class Base 
	{
		public void print()
		{
			System.out.println("BaseClass");
		}
	}
	
	 class Derived extends Base
	{
		public void print()
		{
			System.out.println("Derived");
		}
	}
}
