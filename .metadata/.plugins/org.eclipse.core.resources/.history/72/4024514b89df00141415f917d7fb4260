			public static void main(String[] args) {
				Random rand = new Random(100);
				int Arr[] = new int [10];
				
				for (int i = 0; i < 10; i ++)
					Arr[i] = rand.nextInt(99)+1;
				
				for(int x : Arr)
				System.out.println(x);
				
				
	}
}
			
